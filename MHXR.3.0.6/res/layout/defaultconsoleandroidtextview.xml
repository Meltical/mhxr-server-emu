<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:orientation="vertical" android:background="#ffc4c4c4" android:padding="1.0dip" android:layout_width="fill_parent" android:layout_height="fill_parent"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <LinearLayout android:orientation="vertical" android:id="@id/defaultconsoleandroidtextview_LinearLayout" android:layout_width="fill_parent" android:layout_height="fill_parent">
        <TableLayout android:layout_width="fill_parent" android:layout_height="wrap_content">
            <TableRow>
                <Button android:textColor="#ff000000" android:id="@id/defaultconsoleandroidtextview_buttonCancel" android:background="@drawable/shape" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="キャンセル" android:layout_weight="0.2" />
                <TextView android:visibility="invisible" android:layout_weight="3.0" />
                <Button android:textColor="#ff000000" android:id="@id/defaultconsoleandroidtextview_buttonOK" android:background="@drawable/shape" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="送信" android:layout_weight="0.2" />
            </TableRow>
        </TableLayout>
        <TextView android:layout_width="fill_parent" android:layout_height="1.0dip" />
        <EditText android:textColor="#ff000000" android:id="@id/defaultconsoleandroidtextview_editText" android:background="@drawable/shape" android:layout_width="fill_parent" android:layout_height="wrap_content" android:singleLine="false" />
